using System;

class Program
{
        static void Main()
    {
        // 自分の得意な言語で
        // Let's チャレンジ！！
        var target = Console.ReadLine();
        var str = Console.ReadLine();
        
        int cnt = 0;
        
        for(int i=0; i<str.Length - target.Length+1; i++)
        {
            if(str.Substring(i,target.Length) == target)
            {
                cnt++;
            }
        }
        
        Console.WriteLine(cnt);
    }
}
